<Group labelreference="L2" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">weno_eps := 0;
recon := proc(a) 
   local ijk, beta_shu, weno_coeffs, beta1, beta2, beta3,
        wbarplus1, wbarplus2, wbarplus3, iwbarplussum,
        wplus1, wplus2, wplus3,
        wbarminus1, wbarminus2, wbarminus3, iwbarminussum,
        wminus1, wminus2, wminus3,
        n, A, SQR;
  local aminus, aplus;

  ijk := [-2, -1, 0, +1, +2];

  # 3 so that lists start from 1
  A := proc(i) ; return a[3 + ijk[1+i]] ; end proc;
  SQR := proc(x) ; return x*x ; end proc;

  beta_shu := [[4/3,  -19/3, 25/3, 11/3, -31/3, 10/3 ],
               [4/3,  -13/3, 13/3, 5/3,  -13/3, 4/3 ],
               [ 10/3, -31/3, 25/3, 11/3, -19/3, 4/3 ] ];

  weno_coeffs := [[1/3, -7/6, 11/6, 0,        0 ],
                  [0,   -1/6, 5/6, 1/3 , 0 ],
                  [0,      0, 1/3, 5/6, -1/6]];
  
  beta1  := beta_shu[1+0][1+0]*SQR(A(0)) + beta_shu[0+1][1+1]*A(0)*A(1) +
            beta_shu[1+0][1+2]*SQR(A(1)) + beta_shu[0+1][3+1]*A(0)*A(2) +
            beta_shu[1+0][1+4]*A(1)*A(2) + beta_shu[0+1][5+1]*SQR(A(2));
  beta2  := beta_shu[1+1][1+0]*SQR(A(1)) + beta_shu[1+1][1+1]*A(1)*A(2) +
            beta_shu[1+1][1+2]*SQR(A(2)) + beta_shu[1+1][3+1]*A(1)*A(3) +
            beta_shu[1+1][1+4]*A(2)*A(3) + beta_shu[1+1][5+1]*SQR(A(3));
  beta3  := beta_shu[1+2][1+0]*SQR(A(2)) + beta_shu[2+1][1+1]*A(2)*A(3) +
            beta_shu[1+2][1+2]*SQR(A(3)) + beta_shu[2+1][3+1]*A(2)*A(4) +
            beta_shu[1+2][1+4]*A(3)*A(4) + beta_shu[2+1][5+1]*SQR(A(4));


  wbarplus1 := 1/16 / SQR(weno_eps + beta1);
  wbarplus2 := 5/8 / SQR(weno_eps + beta2);
  wbarplus3 := 5/16 / SQR(weno_eps + beta3);

  iwbarplussum := 1 / (wbarplus1 + wbarplus2 + wbarplus3);

  wplus1 := wbarplus1 * iwbarplussum;
  wplus2 := wbarplus2 * iwbarplussum;
  wplus3 := wbarplus3 * iwbarplussum;
  
  wbarminus1 := 5/16 / SQR(weno_eps + beta1);
  wbarminus2 := 5/8 / SQR(weno_eps + beta2);
  wbarminus3 := 1/16 / SQR(weno_eps + beta3);

  iwbarminussum := 1 / (wbarminus1 + wbarminus2 + wbarminus3);

  wminus1 := wbarminus1 * iwbarminussum;
  wminus2 := wbarminus2 * iwbarminussum;
  wminus3 := wbarminus3 * iwbarminussum;

  aminus := 0; aplus := 0;
  for n from 0 to 4 do
    aplus := aplus + (wplus1 * weno_coeffs[1][1+n] + wplus2 * weno_coeffs[2][1+n] + wplus3 * weno_coeffs[3][1+n]) * A(n);
    aminus := aminus + (wminus1 * weno_coeffs[3][5-n] + wminus2 * weno_coeffs[2][5-n] + wminus3 * weno_coeffs[1][5-n]) * A(n);
  end do; 

  return [aminus, aplus];
end proc;</Text-field>
